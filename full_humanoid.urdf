<?xml version="1.0"?>
<robot name="mini_humanoid"
  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Simple humanoid URDF for RL experiments -->
  <!-- Units: meters, kilograms, radians -->

  <!-- Base / pelvis -->
  <link name="pelvis">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="0.15" ixy="0.0" ixz="0.0" iyy="0.12" iyz="0.0" izz="0.18"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.25 0.15 0.12"/>
      </geometry>
      <material name="pelvis_mat">
        <color rgba="0.6 0.6 0.6 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.25 0.15 0.12"/>
      </geometry>
    </collision>
  </link>

  <!-- Torso -->
  <link name="torso">
    <inertial>
      <origin xyz="0 0 0.12"/>
      <mass value="8.0"/>
      <inertia ixx="0.09" ixy="0.0" ixz="0.0" iyy="0.08" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.12"/>
      <geometry>
        <box size="0.20 0.12 0.24"/>
      </geometry>
      <material name="torso_mat">
        <color rgba="0.4 0.5 0.8 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.12"/>
      <geometry>
        <box size="0.20 0.12 0.24"/>
      </geometry>
    </collision>
  </link>

  <joint name="pelvis_to_torso" type="revolute">
    <parent link="pelvis"/>
    <child link="torso"/>
    <origin xyz="0 0 0.12"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.5" upper="0.5" effort="100" velocity="1"/>
  </joint>

  <!-- Neck & Head -->
  <link name="head">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.08"/>
      <geometry>
        <sphere radius="0.08"/>
      </geometry>
      <material name="head_mat">
        <color rgba="0.9 0.8 0.7 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.08"/>
      <geometry>
        <sphere radius="0.08"/>
      </geometry>
    </collision>
  </link>

  <joint name="torso_to_head" type="revolute">
    <parent link="torso"/>
    <child link="head"/>
    <origin xyz="0 0 0.26"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.5" upper="0.5" effort="20" velocity="1"/>
  </joint>

  <!-- Arms (left and right) -->
  <!-- Upper arm -->
  <link name="left_upper_arm">
    <inertial>
      <origin xyz="0 0 -0.12"/>
      <mass value="0.8"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.12"/>
      <geometry>
        <cylinder length="0.24" radius="0.05"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.12"/>
      <geometry>
        <cylinder length="0.24" radius="0.05"/>
      </geometry>
    </collision>
  </link>
  <link name="left_lower_arm">
    <inertial>
      <origin xyz="0 0 -0.14"/>
      <mass value="0.6"/>
      <inertia ixx="0.0007" iyy="0.0007" izz="0.001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.14"/>
      <geometry>
        <cylinder length="0.28" radius="0.04"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.14"/>
      <geometry>
        <cylinder length="0.28" radius="0.04"/>
      </geometry>
    </collision>
  </link>

  <joint name="torso_to_left_shoulder" type="revolute">
    <parent link="torso"/>
    <child link="left_upper_arm"/>
    <origin xyz="0 0.14 0.18" rpy="0 0 1.5708"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.57" upper="1.57" effort="20" velocity="3.0"/>
  </joint>
  <joint name="left_elbow" type="revolute">
    <parent link="left_upper_arm"/>
    <child link="left_lower_arm"/>
    <origin xyz="0 0 -0.24" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0.0" upper="2.0" effort="15" velocity="3.0"/>
  </joint>

  <!-- Right arm (mirror) -->
  <link name="right_upper_arm">
    <inertial>
      <origin xyz="0 0 -0.12"/>
      <mass value="0.8"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.12"/>
      <geometry>
        <cylinder length="0.24" radius="0.05"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.12"/>
      <geometry>
        <cylinder length="0.24" radius="0.05"/>
      </geometry>
    </collision>
  </link>
  <link name="right_lower_arm">
    <inertial>
      <origin xyz="0 0 -0.14"/>
      <mass value="0.6"/>
      <inertia ixx="0.0007" iyy="0.0007" izz="0.001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.14"/>
      <geometry>
        <cylinder length="0.28" radius="0.04"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.14"/>
      <geometry>
        <cylinder length="0.28" radius="0.04"/>
      </geometry>
    </collision>
  </link>

  <joint name="torso_to_right_shoulder" type="revolute">
    <parent link="torso"/>
    <child link="right_upper_arm"/>
    <origin xyz="0 -0.14 0.18" rpy="0 0 -1.5708"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.57" upper="1.57" effort="20" velocity="3.0"/>
  </joint>
  <joint name="right_elbow" type="revolute">
    <parent link="right_upper_arm"/>
    <child link="right_lower_arm"/>
    <origin xyz="0 0 -0.24" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0.0" upper="2.0" effort="15" velocity="3.0"/>
  </joint>

  <!-- Legs -->
  <link name="left_upper_leg">
    <inertial>
      <origin xyz="0 0 -0.18"/>
      <mass value="3.0"/>
      <inertia ixx="0.02" iyy="0.02" izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.18"/>
      <geometry>
        <cylinder length="0.36" radius="0.06"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.18"/>
      <geometry>
        <cylinder length="0.36" radius="0.06"/>
      </geometry>
    </collision>
  </link>
  <link name="left_lower_leg">
    <inertial>
      <origin xyz="0 0 -0.18"/>
      <mass value="2.2"/>
      <inertia ixx="0.015" iyy="0.015" izz="0.008"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.18"/>
      <geometry>
        <cylinder length="0.36" radius="0.055"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.18"/>
      <geometry>
        <cylinder length="0.36" radius="0.055"/>
      </geometry>
    </collision>
  </link>
  <link name="left_foot">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.8"/>
      <inertia ixx="0.002" iyy="0.002" izz="0.002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.05"/>
      <geometry>
        <box size="0.20 0.10 0.05"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.05"/>
      <geometry>
        <box size="0.20 0.10 0.05"/>
      </geometry>
    </collision>
  </link>

  <joint name="pelvis_to_left_hip" type="revolute">
    <parent link="pelvis"/>
    <child link="left_upper_leg"/>
    <origin xyz="0 0.08 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.5" upper="1.0" effort="100" velocity="4.0"/>
  </joint>
  <joint name="left_knee" type="revolute">
    <parent link="left_upper_leg"/>
    <child link="left_lower_leg"/>
    <origin xyz="0 0 -0.36" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0.0" upper="2.0" effort="150" velocity="4.0"/>
  </joint>
  <joint name="left_ankle" type="revolute">
    <parent link="left_lower_leg"/>
    <child link="left_foot"/>
    <origin xyz="0 0 -0.36" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.5" upper="0.8" effort="50" velocity="4.0"/>
  </joint>

  <!-- Right leg (mirror) -->
  <link name="right_upper_leg">
    <inertial>
      <origin xyz="0 0 -0.18"/>
      <mass value="3.0"/>
      <inertia ixx="0.02" iyy="0.02" izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.18"/>
      <geometry>
        <cylinder length="0.36" radius="0.06"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.18"/>
      <geometry>
        <cylinder length="0.36" radius="0.06"/>
      </geometry>
    </collision>
  </link>
  <link name="right_lower_leg">
    <inertial>
      <origin xyz="0 0 -0.18"/>
      <mass value="2.2"/>
      <inertia ixx="0.015" iyy="0.015" izz="0.008"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.18"/>
      <geometry>
        <cylinder length="0.36" radius="0.055"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.18"/>
      <geometry>
        <cylinder length="0.36" radius="0.055"/>
      </geometry>
    </collision>
  </link>
  <link name="right_foot">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.8"/>
      <inertia ixx="0.002" iyy="0.002" izz="0.002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.05"/>
      <geometry>
        <box size="0.20 0.10 0.05"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.05"/>
      <geometry>
        <box size="0.20 0.10 0.05"/>
      </geometry>
    </collision>
  </link>

  <joint name="pelvis_to_right_hip" type="revolute">
    <parent link="pelvis"/>
    <child link="right_upper_leg"/>
    <origin xyz="0 -0.08 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.5" upper="1.0" effort="100" velocity="4.0"/>
  </joint>
  <joint name="right_knee" type="revolute">
    <parent link="right_upper_leg"/>
    <child link="right_lower_leg"/>
    <origin xyz="0 0 -0.36" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0.0" upper="2.0" effort="150" velocity="4.0"/>
  </joint>
  <joint name="right_ankle" type="revolute">
    <parent link="right_lower_leg"/>
    <child link="right_foot"/>
    <origin xyz="0 0 -0.36" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.5" upper="0.8" effort="50" velocity="4.0"/>
  </joint>

  <!-- Floating base hint: some physics engines expect a fixed base; for RL set free by loading without fixed base -->
</robot>
